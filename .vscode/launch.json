{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch API",
      "cwd": "${workspaceFolder}/api",
      "program": "/usr/local/bin/serverless",
      "args": ["offline", "start"]
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "Launch Chrome against localhost angular",
      "url": "http://localhost:4200",
      "webRoot": "${workspaceFolder}/website"
    }
  ]
}
